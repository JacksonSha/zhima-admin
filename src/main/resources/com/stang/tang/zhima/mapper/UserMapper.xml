		<result column="USR_EMAIL" property="email" jdbcType="VARCHAR" />
		<result column="USR_NICKNAME" property="nickName" jdbcType="VARCHAR" />  
		<result column="USR_PASSWORD" property="password" jdbcType="VARCHAR" />
		<result column="USR_HEADIMG" property="headImg" jdbcType="VARCHAR" />
		<result column="USR_FROM" property="from" jdbcType="VARCHAR" />
		<result column="USR_PROFILE" property="profile" jdbcType="VARCHAR" />
		<result column="USR_REGISTER_DT" property="registerDt" jdbcType="DATE" />
	</resultMap>

	<sql id="ColumnList">
		USR_ID,USR_EMAIL,USR_NAME,USR_NICKNAME,USR_PASSWORD,USR_HEADIMG,USR_FROM,USR_PROFILE,USR_REGISTER_DT
